	<queryString>
		<![CDATA[SELECT
     venta.`VentaID` AS VentaID,
     venta.`FechaVenta` AS FechaVenta,
     venta.`Subtotal` AS Subtotal,
     venta.`Iva` AS Iva,
     venta.`PrecioTotal` AS PrecioTotal,
     detalleventa.`Cantidad` AS detalleventa_Cantidad,
     producto.`Nombre` AS producto_Nombre,
     producto.`Precio` AS producto_Precio,
     producto.`Existencia` AS producto_Existencia,
     producto.`Descripcion` AS producto_Descripcion,
     tipoproducto.`Nombre` AS tipoproducto_Nombre,
     producto.`ProductoID` AS producto_ProductoID
FROM
     `venta` venta INNER JOIN `detalleventa` detalleventa ON venta.`VentaID` = detalleventa.`VentaID`
     INNER JOIN `producto` producto ON detalleventa.`ProductoID` = producto.`ProductoID`
     INNER JOIN `tipoproducto` tipoproducto ON producto.`TipoProductoID` = tipoproducto.`TipoProductoID`
WHERE venta.VentaID = $P!{id_venta}]]>
	</queryString>
